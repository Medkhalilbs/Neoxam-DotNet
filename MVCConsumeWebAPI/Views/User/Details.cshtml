                        @{String path = "http://localhost:18080/gestion-resources-humaine-web/uploads/" + Model.picture;}  
                        <img src=@Url.Content(path) class="rounded-circle" width="150" />

                        <h4 class="card-title m-t-10">@Html.DisplayFor(model => model.first_name) @Html.DisplayFor(model => model.last_name)</h4>
                        <h6 class="card-subtitle">@Html.DisplayFor(model => model.role) chez Neoxam Tunisie</h6>
                        <div class="row text-center justify-content-md-center">
                            <div class="col-4"><a href="javascript:void(0)" class="link"><i class="icon-people"></i> <font class="font-medium">254</font></a></div>
                            <div class="col-4"><a href="javascript:void(0)" class="link"><i class="icon-picture"></i> <font class="font-medium">54</font></a></div>
                        </div>
                    </center>
                </div>
                <div>
                    <hr>
                </div>
                <div class="card-body">

                    <small class="text-muted">Nom d'utilisateur</small><h6>@Html.DisplayFor(model => model.login)</h6> 
                    <small class="text-muted">Adresse Email</small><h6>@Html.DisplayFor(model => model.email)</h6> 
                    <small class="text-muted p-t-30 db">N° mobile</small><h6>@Html.DisplayFor(model => model.phone_number)</h6> 
                    <small class="text-muted">N° Carte d'Identité Nationale</small><h6>@Html.DisplayFor(model => model.cin)</h6>
                    <small class="text-muted p-t-30 db">Etat</small>
                    <h6>

                            @if (Model.status.Equals("pending"))
                            { <span class='label label-info'>En attente</span>}
                            else if (Model.status.Equals("active"))
                            { <span class='label label-success'>Activé</span>}
                            else if (Model.status.Equals("blocked"))
                            { <span class='label label-danger'>Bloqué</span>}
                        
                        
                    </h6> 
                    <small class="text-muted p-t-30 db">Date de creation du compte</small><h6>@Html.DisplayFor(model => model.registration_date)</h6> 
                    <small class="text-muted p-t-30 db">Adresse</small><h6>@Html.DisplayFor(model => model.address.addressLine1), @Html.DisplayFor(model => model.address.addressLine2) @Html.DisplayFor(model => model.address.city) @Html.DisplayFor(model => model.address.state), @Html.DisplayFor(model => model.address.zipCode), @Html.DisplayFor(model => model.address.country)</h6>
                    <div class="map-box">
                        @{String path2 = "https://maps.google.com/maps?q=" + Model.address.lat.ToString("G", System.Globalization.CultureInfo.InvariantCulture) + "," + Model.address.lng.ToString("G", System.Globalization.CultureInfo.InvariantCulture) + "&z=14&output=embed";}
                        <iframe src=@path2 width="100%" height="150" frameborder="0" style="border:0" allowfullscreen></iframe>
                    </div> <small class="text-muted p-t-30 db">Profile sociale</small>
                    <br />
                    <a class="btn btn-circle btn-secondary" href="http://www.facebook.com/ladnene"><i class="fab fa-facebook-f"></i></a>
                    <a class="btn btn-circle btn-secondary" href="http://www.facebook.com/ladnene"><i class="fab fa-twitter"></i></a>
                    <a class="btn btn-circle btn-secondary" href="http://www.facebook.com/ladnene"><i class="fab fa-youtube"></i></a>
</div>
            </div>
        </div>
        <!-- Column -->
        <!-- Column -->
        <div class="col-lg-8 col-xlg-9 col-md-7">
            <div class="card">
                <!-- Tabs -->
                <ul class="nav nav-pills custom-pills" id="pills-tab" role="tablist">
                    <li class="nav-item">
                        <a class="nav-link active" id="pills-timeline-tab" data-toggle="pill" href="#current-month" role="tab" aria-controls="pills-timeline" aria-selected="true">Timeline</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" id="pills-profile-tab" data-toggle="pill" href="#last-month" role="tab" aria-controls="pills-profile" aria-selected="false">Profile</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" id="pills-setting-tab" data-toggle="pill" href="#previous-month" role="tab" aria-controls="pills-setting" aria-selected="false">Setting</a>
                    </li>
                </ul>
                <!-- Tabs -->
                <div class="tab-content" id="pills-tabContent">
                    <div class="tab-pane fade show active" id="current-month" role="tabpanel" aria-labelledby="pills-timeline-tab">
                        <div class="card-body">
                           
                        </div>
                    </div>
                    <div class="tab-pane fade" id="last-month" role="tabpanel" aria-labelledby="pills-profile-tab">
                        <div class="card-body">
                            <div class="row">
                                <div class="col-md-3 col-xs-6 b-r">
                                    <strong>Full Name</strong>
                                    <br>
                                    <p class="text-muted">Johnathan Deo</p>
                                </div>
                                <div class="col-md-3 col-xs-6 b-r">
                                    <strong>Mobile</strong>
                                    <br>
                                    <p class="text-muted">(123) 456 7890</p>
                                </div>
                                <div class="col-md-3 col-xs-6 b-r">
                                    <strong>Email</strong>
                                    <br>
                                    <p class="text-muted">johnathan@admin.com</p>
                                </div>
                                <div class="col-md-3 col-xs-6">
                                    <strong>Location</strong>
                                    <br>
                                    <p class="text-muted">London</p>
                                </div>
                            </div>
                            <hr>
                            <p class="m-t-30">Donec pede justo, fringilla vel, aliquet nec, vulputate eget, arcu. In enim justo, rhoncus ut, imperdiet a, venenatis vitae, justo. Nullam dictum felis eu pede mollis pretium. Integer tincidunt.Cras dapibus. Vivamus elementum semper nisi. Aenean vulputate eleifend tellus. Aenean leo ligula, porttitor eu, consequat vitae, eleifend ac, enim.</p>
