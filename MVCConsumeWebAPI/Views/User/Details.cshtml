@model MVCConsumeWebAPI.Models.AdministratorRead
@using MVCConsumeWebAPI.Extensions
@{
    ViewBag.Title = "Details";
}
<h2>Details profile</h2>
<hr />
<div class="container-fluid">
    <!-- ============================================================== -->
    <!-- Start Page Content -->
    <!-- ============================================================== -->
    <!-- Row -->
    <div class="row">
        <!-- Column -->
        <div class="col-lg-4 col-xlg-3 col-md-5">
            <div class="card">
                <div class="card-body">
                    <center class="m-t-30">
                        @*@{String path = "~/Images/Uploads/Users/Pictures/" + Model.picture;}*@
                        @*@if (Model.id == User.Identity.GetID())
                        { path = "~/Images/Uploads/Users/Pictures/" + Model.picture; }
